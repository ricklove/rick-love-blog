  "version": "0.1.0",
  "author": "Rick Love <rick@toldpro.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ricklove/rick-love-blog"
  },
  "bugs": {
    "url": "https://github.com/ricklove/rick-love-blog/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
